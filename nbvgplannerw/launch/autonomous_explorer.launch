<launch>
  <env name="ROSCONSOLE_CONFIG_FILE" value="$(find nbvgplannerww)/resource/configuration.conf"/>
  <arg name="robot_namespace" default="/"/>
  <arg name="move_forward_only" default="False"/>
  <arg name="param_file" default="$(find nbvgplannerww)/resource/exploration.yaml" />
  <arg name="maxTime" default="300"/>

  <!-- Autonomous Explorer Node -->
  <node pkg="nbvgplannerww" type="nbvgplannerwwNode" name="nbvgplannerww" output="screen">
    <rosparam command="load" file="$(arg param_file)" />
  </node>

  <node pkg="nbvgplannerww" type="nbvgplannerwwMain" name="nbvgplannerwwMain" output="screen">
    <param name="maxTime" value="$(arg maxTime)" />
  </node>

  <node pkg="tf" type="static_transform_publisher" name="tf_weight" args="0 0 0 0 0 0 map weight 10" />

</launch>
